class Time
{
  int hours;
 int minutes;
 int seconds;
 Time(int HH)
{
  hours=HH;
}
  Time(int HH,int MM)
{
 hours=HH;
 minutes=MM;
}
  Time(int HH,int MM,int SS)
{
  hours=HH;
  minutes=MM;
  seconds=SS;
}
 void displayInfo()
{
  System.out.println("Hours:"+hours+" "+"Minutes:"+minutes+" "+"Seconds:"+seconds);
}
public static void main(String args[])
{
 Time t1=new Time(12 );
 Time t2=new Time(1,4 );
 Time t3=new Time(7,20,5);
 t1.displayInfo();
 t2.displayInfo();
 t3.displayInfo();
}
}
